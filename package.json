{
  "name": "gym-backup-workflow",
  "version": "1.0.0",
  "description": "Automated backup system for gym management database",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/backup.ts",
    "generate-tokens": "node generate-new-tokens.js",
    "test:env": "node tests/test-env.js",
    "test:drive": "node tests/test-drive.js",
    "test:email": "node tests/test-email.js",
    "test:supabase": "node tests/test-supabase.js",
    "test:retry": "node tests/test-retry.js",
    "test:error": "node tests/test-error-handling.js",
    "test:exporter": "node tests/test-exporter.js",
    "test:all": "npm run test:env && npm run test:drive && npm run test:email",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "googleapis": "^140.0.1",
    "nodemailer": "^6.9.14",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^6.4.15",
    "@types/xlsx": "^0.0.35",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "backup",
    "database",
    "automation",
    "supabase"
  ],
  "author": "",
  "license": "ISC"
}
